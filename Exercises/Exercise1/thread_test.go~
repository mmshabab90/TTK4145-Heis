
package main

import(
	. "fmt"
	"runtime"
	"time"
)

func thread1foo(){
	for j := 0; j < 10000; j++{
		i++
	}
}

func thread2foo(){
	for j := 0; j < 10000; j++{
		i--
	}
}

func main() {

	int i = 0

	runtime.GOMAXPROCS(runtime.NumCPU())

	go thread1foo()

	go thread2foo()

	time.sleep(100*time.Millisecond)
	
	Println(i)
}
